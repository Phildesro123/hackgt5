<link rel="stylesheet" href="..public/css/login.css">
<h2>Flight Attendent Sign-In</h2>
<div class="well container" id="shrink">
<form method="post" action="/login">
    <div class="cred-div">
        <label>Username:</label>
        <input type="text" value="{{user_name}}" name="username"><br>
    </div>
    <div class="cred-div">
        <label>Password:</label>
        <input type="password" value='{{user_password}}' name="password"><br>
    </div>
    <input type="submit">

</form>
</div>
